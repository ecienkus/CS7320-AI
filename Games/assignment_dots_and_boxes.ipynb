    "    ax.set_ylim(-0.5, n-0.5)\n",
    "\n",
    "    # Draw dots\n",
    "    for row in range(n):\n",
    "        for col in range(m):\n",
    "            if (row, col) in board:\n",
    "                ax.scatter(row, col, s=100, marker='o', color='black')\n",
    "            else:\n",
    "                ax.scatter(row, col, s=100, marker='o', color='black')\n",
    "\n",
    "    # Draw lines\n",
    "    for line in board:\n",
